{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-trailing-whitespace": false,
    "semicolon": [
      true,
      "always",
      "strict-bound-class-methods"
    ],
    "no-console": false,
    "object-literal-sort-keys": false,
    "no-empty": [
      true,
      "allow-empty-functions"
    ],
    "interface-name": false
  },
  "rulesDirectory": []
}
